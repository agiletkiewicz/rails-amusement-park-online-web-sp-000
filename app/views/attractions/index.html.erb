<h1> Listing attractions </h1>

<table>
<tbody>
    <tr>
        <td>Name</td>
        <td>Tickets</td>
        <td>Details</td>
    </tr>
    <% @attractions.each do |att| %>
        <tr>
            <td><%= att.name %></td>
            <td><%= att.tickets %></td>
            <td><%= link_to 'Go on #{att.name}', attraction_path(att) %></td>
        </tr>
    <% end %>           


</tbody>
</table>